{
  "env": {
    "browser": true,
    "es2021": true,
    "commonjs": true,
    "es6": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": [
      "warn",
      {
        "singleQuote": true,
        "semi": false,
        "trailingComma": "none",
        "endOfLine": "false"
      }
    ],
    "indent": ["error", 2],
    "no-console": "off"
  }
}
